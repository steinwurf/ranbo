{
  "name": "ranbo",
  "version": "2.0.0",
  "description": "A C Pseudo-Random-Number-Generator (PRNG) library.",
  "keywords": [
    "random"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/steinwurf/ranbo.git"
  },
  "export": {
    "exclude": [
      ".github/**",
      "benchmark/**",
      "docs/**",
      "examples/**",
      "test/**",
      ".clang-format",
      ".gitignore",
      "CMakeLists.txt",
      "giit.json",
      "resolve.json",
      "waf",
      "wscript"
    ]
  },
  "build": {
    "srcFilter": [
      "+<*>"
    ],
    "srcDir": "src",
    "includeDir": "src"
  },
  "headers": [
    "ranbo.h",
    "rand48.h",
    "xoshiro64s.h",
    "xoshiro256ss.h"
  ],
  "frameworks": "*",
  "platforms": "*",
  "dependencies": []
}